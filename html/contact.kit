		<!-- @include 'nav.kit' -->

		<main>
			<section class="map">
				<div id="map"></div>
			</section>

			<section class="email">
				<div class="wrapper">
					<div class="section-heading">
						<h2>KONTAKT</h2>
					</div>

					<form action="">
						<div class="email__items form__inputs">
							<div class="email__items-item">
								<label for="name">Imię:</label>
								<input type="text" id="name" />
							</div>

							<div class="email__items-item">
								<label for="email">Email:</label>
								<input type="text" id="email" />
							</div>
						</div>

						<div class="form__items form__inputs">
							<label for="topic">Tytuł:</label>
							<input type="text" id="topic" />
						</div>

						<div class="form__items form__inputs">
							<label for="name">Treść:</label>
							<textarea id="name"></textarea>
						</div>

						<div class="button">
							<button type="submit">wyślij</button>
						</div>
					</form>
				</div>
			</section>
		</main>

		<!-- @include 'footer.kit' -->

		<script>
			feather.replace();
		</script>
